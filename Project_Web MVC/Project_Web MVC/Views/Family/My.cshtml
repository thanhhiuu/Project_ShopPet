<h2>Giới thiệu về bản thân</h2>
<hr />


<div class="row">
    <div class="col-4">
        <div class="card">
            <div class="card-header">
                <img class="img-fluid" src="~/uploads/@ViewBag.Hinh" />
            </div>
            <div class="card-body">
                <ul>
                    <li>Họ tên: @ViewBag.HoTen</li>
                    <li>Tuổi: @ViewBag.Tuoi</li>
                    <li>Nghề nghiệp: @ViewBag.NgheNghiep</li>

                </ul>
            </div>
        </div>
    </div>
</div>


